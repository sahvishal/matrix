@using Falcon.App.UI.HtmlHelpers
@model Falcon.App.Core.CallQueues.ViewModels.CallQueueEditModel
@{
    ViewBag.Title = "Edit Call Queue";
}
<h1>
    Edit Call Queue
</h1>
<div class="option-links">
    <a href="/CallCenter/CallQueue">Manage Call Queue </a>
</div>
@if (Model != null)
{
    @Html.Partial("Message", new ViewDataDictionary(Model.FeedbackMessage))
}
@{Html.BeginForm();}
@Html.EditorForModel()
<div class="submit-button-row">
    <a href="/CallCenter/CallQueue">Close</a> &nbsp;&nbsp; @Html.SubmitButton("Edit Call Queue")
</div>
@{Html.EndForm();}