@using Falcon.App.UI.HtmlHelpers
@model Falcon.App.Core.Medical.ViewModels.HealthAssessmentTemplateEditModel
@{
    ViewBag.Title = "Create";
}

<h1>Create Health Assessment Template</h1>
<div class="option-links">
    <a href="/Medical/HealthAssessment/">Manage Health Assessment Template </a>
</div>
@if (Model != null)
{
    @Html.Partial("Message", new ViewDataDictionary(Model.FeedbackMessage))
}
@{Html.BeginForm("Create", "HealthAssessment");} 
@Html.EditorForModel()
<div class="submit-button-row">
    @Html.ActionLink("Close", "Index", "HealthAssessment") &nbsp;&nbsp; @Html.SubmitButton("Create Template")
</div>
@{Html.EndForm();}

