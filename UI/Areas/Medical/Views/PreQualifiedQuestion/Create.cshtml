@using Falcon.App.UI.HtmlHelpers
@model Falcon.App.Core.Medical.ViewModels.PreQualifiedQuestionTemplateEditModel
@{
    ViewBag.Title = "Create PreQualified Question Template";
}

<h1>Create PreQualified Question Template</h1>
<div class="option-links">
    <a href="/Medical/PreQualifiedQuestion/">Manage PreQualified Question Template</a>
</div>
@if (Model != null)
{
    @Html.Partial("Message", new ViewDataDictionary(Model.FeedbackMessage))
}
@{Html.BeginForm("Create", "PreQualifiedQuestion");}

@Html.EditorForModel()
<div class="submit-button-row">
    @Html.ActionLink("Close", "Index", "PreQualifiedQuestion") &nbsp;&nbsp; <input type="button" value="Submit" onclick="SubmitQuestions()" />
    <div style="display: none;">@Html.SubmitButton("Create Template")</div>
</div>
@{Html.EndForm();}

