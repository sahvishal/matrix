<script language="javascript" type="text/javascript">

    var selectedRecords = new Object();
    var record = new Object();
    var key_haf = 1;
    var setDefaultAnswer = false;
    var isHkynPurchased = '@Model.IsHKynPurchased' == '@Boolean.TrueString' ? true : false;
    
    var isMyBioCheckPurchased = '@Model.IsBioCheckAssessmentPurchased' == '@Boolean.TrueString' ? true : false;

    $(document).ready(function () {
        @if(!htmlPageBreakBox)
		{
			<text>
        $(".sequence-block input[type='radio'], .sequence-block input[type='checkbox']").click(function () {
            onEventRaisedbyControl($(this));
        });

        $(".sequence-block input[type='text'].text-question, .sequence-block textarea:visible").change(function () {
            onEventRaisedbyControl($(this));
        });
        </text>
		}

        $(".questionid").closest(".sequence-block").hide();

        @{
            var count = Model.QuestionEditModels.Where(m => !string.IsNullOrEmpty(m.Answer)).Count();

            //if (count > 0)
            //{
              //  <text> setDefaultAnswer = true; </text>
            //}

            foreach (var item in Model.QuestionEditModels)
            {
                if (item.QuestionId == 100 && Model.Gender != Falcon.App.Core.Users.Enum.Gender.Female && Model.CustomerId > 0)
                {
                    continue;
                }
                var answer = !string.IsNullOrEmpty(item.Answer) ? item.Answer.Replace("'", "\\\'").Replace("\"", "\\\"").Replace("\r", " ").Replace("\n", "{newlinefeed}") : "";

                if ((item.ControlType == (int) DisplayControlType.Radio || item.ControlType == (int) DisplayControlType.CheckBox))
                {
                    answer = !string.IsNullOrEmpty(item.Answer) ? item.Answer.Replace("'", "\\\\\'").Replace("\"", "\\\"").Replace("\r", " ").Replace("\n", "{newlinefeed}") : "";
                }

                <text>
        setControlType('@item.QuestionId', "@Html.Raw(answer)", '@item.DefaultAnswer', '@item.ControlType');
        addObject('@item.QuestionId', "@Html.Raw(answer)", '@item.DefaultAnswer', '@item.ControlType');
        </text>
            }

			if(!htmlPageBreakBox)
			{
				<text>
        renderTemplate();
        setQuestionDependency();
        $("input[type=radio][name='question-47']").bind("click", function () { setRatings(); });
        </text>
            }
        }

        @if (Model.IsMammoPurchased && htmlPageBreakBox && showMammogramQuestionnarire)
        {
            <text>setMammoQuestions();</text>
        }

        @if (Model.IsKynPurchased || Model.IsBioCheckAssessmentPurchased)
        {
            <text>EnableDisableWomenOnlyKyn();</text>

            if (htmlPageBreakBox)
            {
                <text>setKynQuestions();</text>
            }

        }

        @if (Model.IsPhq9Purchased)
        {
            if (htmlPageBreakBox)
            {
                <text>setphqQuestions();</text>
            }
        }

        $(".customer-gender").change(function () {
            if ($(this).val() == "@((int)Gender.Female)") {
                EnableKynWomenOnly(true);
            } else {
                EnableKynWomenOnly(false);
            }
        });

    });

    function setControlType(questionId, answer, defAnswer, controlType) {//debugger;
        var parentDiv = $("input[type=text].questionid[value='" + questionId + "']").parents(".sequence-block:first");
        if (parentDiv.length < 1) return;

        var defanswer_input = parentDiv.find("input[type=text].default-answer");
        var controltype_input = parentDiv.find("input[type=text].control-type");

        defanswer_input.val(defAnswer);
        controltype_input.val(controlType);


        if (answer != null && $.trim(answer).length > 0) {
            setAnswertoControl(parentDiv, controlType, answer);
        }
        else if (setDefaultAnswer == true && '@isBulkPrint' == '@Boolean.FalseString') {
            setAnswertoControl(parentDiv, controlType, defAnswer);
        }
    }

    function EnableDisableWomenOnlyKyn() {
        @if (Model.Gender == Gender.Female)
        {
            <text>EnableKynWomenOnly(true);</text>
        }
        else
        {
            <text>EnableKynWomenOnly(false);</text>
        }
    }



    function setAnswertoControl(parentDiv, controltype, answer) {//debugger;
        if (controltype == '@((int)DisplayControlType.Radio)') {
            parentDiv.find("input[type='radio'][ctrl_answer='" + answer + "']").attr("checked", true);
        }
        else if (controltype == '@((int)DisplayControlType.CheckBox)') {
            parentDiv.find("input[type='checkbox'][ctrl_answer='" + answer + "']").attr("checked", true);
        }
        else if (controltype == '@((int)DisplayControlType.TextBox)') {
            parentDiv.find("input[type='text'].text-question").val(answer);
        }
        else if (controltype == '@((int)DisplayControlType.TextArea)') {
            parentDiv.find("textarea").val(answer.replace(/{newlinefeed}/gi, "\n"));
        }
    }

    function onEventRaisedbyControl(currentControlReference) {//debugger;
        var parentDiv = currentControlReference.parents(".sequence-block:first");

        var questionId = parentDiv.find("input[type=text].questionid").val();
        var defanswer = parentDiv.find("input[type=text].default-answer").val();
        var controltype = parentDiv.find("input[type=text].control-type").val();
        var answer = null;

        var ctrlTypeTArea = false;

        if (controltype == '@((int)DisplayControlType.Radio)' || controltype == '@((int)DisplayControlType.CheckBox)') {
            if (currentControlReference.attr("checked") == true) {
                answer = currentControlReference.attr("ctrl_answer");
            }
        }
        else if (controltype == '@((int)DisplayControlType.TextBox)') {
            answer = $.trim(currentControlReference.val());
        }
        else if (controltype == '@((int)DisplayControlType.TextArea)') {
            ctrlTypeTArea = true;
            answer = $.trim(currentControlReference.val());
        }

        addObject(questionId, answer, defanswer, controltype);

        renderTemplate();

        if (ctrlTypeTArea) { //debugger;
            var answerCtrl = $("#healthAssessmentTemplateDiv input[value='" + questionId + "']").next();
            answerCtrl.val(answer);
        }
        if ($("input[type=radio][name='question-440'][ctrl_answer='No']").attr("checked") || $("input[type=radio][name='question-440'][ctrl_answer='Yes']").attr("checked") == false) {
            SetEnableDisableSingleQuetionsRadiobtn(441, false);
        }
        if ($("input[type=radio][name='question-26'][ctrl_answer='No']").attr("checked") || $("input[type=radio][name='question-26'][ctrl_answer='Yes']").attr("checked") == false) {
            SetEnableDisableSingleQuetionsRadiobtn(444, false);
        }
    }



    function addObject(questionId, answer, defAnswer, controlType) {

        var parentDiv = $("input[type=text].questionid[value='" + questionId + "']").parents(".sequence-block:first");
        parentDiv.show();

        var key = searchCollection(questionId);

        if ((controlType == '@((long)DisplayControlType.TextBox)' || controlType == '@((long)DisplayControlType.TextArea)') && (answer != null || $.trim(answer).length > 1)) {
            answer = answer.replace(/\?\?+/gi, "?");
        }

        if (key >= 0) {
            record = selectedRecords[key];
        }
        else {
            record = new Object();
        }

        if (answer == null || $.trim(answer).length < 1 || $.trim(answer) == defAnswer) {
            if (key >= 0) {
                removeObject(key);
            }
            return;
        }

        record.QuestionId = questionId;
        record.Answer = answer;
        record.DefaultAnswer = defAnswer;
        record.ControlType = controlType;

        if (key >= 0) {
            selectedRecords[key] = record;
        }
        else {
            record.Key = key_haf;
            selectedRecords[key_haf] = record;
            key_haf = key_haf + 1;
        }

    }

    function getHealthAssessmentModeltoSave() {
        var recordsArray = new Array();

        $.each(selectedRecords, function () {
            recordsArray.push(this);
        });

        return { 'CustomerId': '@Model.CustomerId', 'EventId': '@Model.EventId', 'QuestionEditModels': recordsArray };
    }

</script>

<script id="selectedQuestionTemplate" type="text/x-jquery-tmpl">
    <input type="hidden" name="QuestionEditModels.Index" class='selectedquestion-key' value="${Key}" />
    <input type="text" name="QuestionEditModels[${Key}].QuestionId" value="${QuestionId}" />
    {{if ControlType == '@((int)DisplayControlType.TextArea)'}}
    <textarea name="QuestionEditModels[${Key}].Answer">${Answer}</textarea>
    {{else}}
    <input type="text" name="QuestionEditModels[${Key}].Answer" value="${Answer}" />
    {{/if}}
    <input type="text" name="QuestionEditModels[${Key}].DefaultAnswer" value="${DefaultAnswer}" />
    <input type="text" name="QuestionEditModels[${Key}].ControlType" value="${ControlType}" />
</script>
