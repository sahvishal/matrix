    var showMammogramQuestionnarire = ViewData["ShowMammogramQuestionnarire"] != null && Convert.ToBoolean(ViewData["ShowMammogramQuestionnarire"]) == true ? true : false;

    var noOfQuestionsInOnePage = 31;
    var noOfBioCheckQuestionsInOnePage = 24;
    var bioCheckQuestionCount = Model.QuestionEditModels.Count(x => Model.bioCheckQuestionIds.Contains(x.QuestionId));
    var hasHcpClinicalQuestions = Model.QuestionEditModels.Any(x => Model.hcpClinicalQuestionIds.Contains(x.QuestionId));

    var displayMainDiv = "";
    var hasQuestionsOtherThanHcpClinical = Model.QuestionEditModels.Any(x => !Model.hcpClinicalQuestionIds.Contains(x.QuestionId));
    if (!hasQuestionsOtherThanHcpClinical)
    {
        displayMainDiv = "style=display:none;";
    }

    var showPhqHeader = Model.QuestionEditModels.Any(x => Model.phq9QuestionIds.Contains(x.QuestionId));//((Model.IsMammoPurchased && !mammogramQuestionIds.Contains(q.QuestionId)) || (!Model.IsMammoPurchased))
    var noOfQuestions = Model.QuestionEditModels.Count(q => !Model.mammogramQuestionIds.Contains(q.QuestionId)
                        && !Model.kynQuestionIds.Contains(q.QuestionId) && !Model.phq9QuestionIds.Contains(q.QuestionId)
                        && !Model.hcpClinicalQuestionIds.Contains(q.QuestionId) && !Model.hcpSecondaryRiskAssessmentQuestions.Contains(q.QuestionId)
                        && !Model.hcpSecondaryImmunizations.Contains(q.QuestionId) && !Model.levelOneQuestions_Parent.Contains(q.QuestionId)
                        && !Model.bioCheckQuestionIds.Contains(q.QuestionId));


    var isAnyHcpSecondayQuestions = Model.QuestionEditModels.Any(x => Model.hcpSecondaryRiskAssessmentQuestions.Contains(x.QuestionId) || Model.hcpSecondaryImmunizations.Contains(x.QuestionId));
    var isAnyImmunizationQuestions = Model.QuestionEditModels.Any(x => Model.hcpSecondaryImmunizations.Contains(x.QuestionId));
    var isAnyRiskAssessmentQuestions = Model.QuestionEditModels.Any(x => Model.hcpSecondaryImmunizations.Contains(x.QuestionId));

    var hasLevelOneQuestions = Model.QuestionEditModels.Any(x => Model.levelOneQuestions_Parent.Contains(x.QuestionId));


    var hasMammoQuestionsWithoutMammoTest = Model.QuestionEditModels.Any(q => !Model.IsMammoPurchased && Model.mammogramQuestionIds.Contains(q.QuestionId));


    string serveralDaysPhqOptions = "several days";
    string morethanhalfdaysPhqOptions = "more than half the days";
    string nearlyEveryDayPhqOptions = "nearly every day";
    int phqSeveralDaysAnswerCount = 0;
    int moreThanHalfAnswerCount = 0;
    int nearlyEveryDaysAnswerCount = 0;

    if (Model.IsPhq9Purchased)
    {
        phqSeveralDaysAnswerCount = Model.QuestionEditModels.Count(x => Model.phq9QuestionIds.Contains(x.QuestionId) && !string.IsNullOrEmpty(x.Answer) && x.Answer.Trim().ToLower() == serveralDaysPhqOptions);
        moreThanHalfAnswerCount = Model.QuestionEditModels.Count(x => Model.phq9QuestionIds.Contains(x.QuestionId) && !string.IsNullOrEmpty(x.Answer) && x.Answer.Trim().ToLower() == morethanhalfdaysPhqOptions);
        nearlyEveryDaysAnswerCount = Model.QuestionEditModels.Count(x => Model.phq9QuestionIds.Contains(x.QuestionId) && !string.IsNullOrEmpty(x.Answer) && x.Answer.Trim().ToLower() == nearlyEveryDayPhqOptions);
    }

    if (hasHcpClinicalQuestions || isAnyHcpSecondayQuestions)
    {
        noOfQuestionsInOnePage = 40;
    }

    var showEthinicGroup = ViewData["ShowEthnicGroup"] != null && Convert.ToBoolean(ViewData["ShowEthnicGroup"]);
    var isBulkPrint = ViewData["IsBulkPrint"] != null && Convert.ToBoolean(ViewData["IsBulkPrint"]);
}
