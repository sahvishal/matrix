<script type="text/javascript">

    var checkList = [];
    function txtkeypress(evt) {
        return KeyPress_NumericAllowedOnly(evt);
    }

    function setChildQuestionStatus(questionId, evt) {
        var childQuestions = $("span[parentId='" + questionId + "']");
        var inputBox = $(childQuestions).find("input[type='text']");

        if ($(evt).is(":checked")) {
            $(inputBox).val('');
            $(inputBox).removeAttr("disabled");
        } else {
            $(inputBox).val('');
            $(inputBox).attr("disabled", true);

        }
    }

    function setRadioChildQuestionStatus(questionId, evt, defaultValue) {
        var childQuestions = $("span[parentId='" + questionId + "']");
        var chckboxs = $(childQuestions).find("input[type='checkbox']");
        var inputBox = $(childQuestions).find("input[type='text']");
        var radiobtns = $(childQuestions).find("input[type='radio']");

        if ($(evt).val().toLowerCase() === defaultValue.toLowerCase()) {
            $(chckboxs).removeAttr("disabled");
            $(inputBox).val('');
            $(inputBox).removeAttr("disabled");
            $(radiobtns).removeAttr("disabled");
        } else {
            $(chckboxs).attr("checked", false);
            $(chckboxs).attr("disabled", true);
            $(inputBox).val('');
            $(inputBox).attr("disabled", true);
            $(radiobtns).attr("checked", false);
            $(radiobtns).attr("disabled", true);
        }
    }

    function setDisabled() {
        $("#checklisForm-container").find("input").attr("disabled", true);
    }

    function CopyHeaderInfoForPrint() {
        $(".printable-Header").html($("#headerInfo").html());
    }

    function setQuestionStatus(questionId, answer, questionType) {
        var span = $("span[questionId='" + questionId + "']:visible");
        var childQuestions = $("span[parentId='" + questionId + "']");

        if (questionType == '@CheckListQuestionType.CheckBox') {
            var checkBox = $(span).find("input[type='checkbox']");
            if (answer.toLowerCase() == 'yes') {
                $(checkBox).attr("checked", true);
                setParentQuestionUnchecked(questionId);
            }

            if (childQuestions.length > 0) {
                setChildQuestionStatus(questionId, checkBox);
            }

        } else if (questionType == '@CheckListQuestionType.Radio') {

            var radio = $(span).find("input[name='questionid-" + questionId + "']");

            if (answer !== '') {
                var values = answer.toLowerCase();
                radio = $(span).find("input[name='questionid-" + questionId + "'][value=" + values + "]");
                $(radio).attr("checked", true);
            }


            if (childQuestions.length > 0) {
                //debugger;
                var defaultValue = $(span).attr('default');
                setRadioChildQuestionStatus(questionId, radio, defaultValue);
            }
        }
        else if (questionType == '@CheckListQuestionType.TextBox') {
            $(span).find("input[type='text']").val(answer);
        }
    }

    function showHideQuestion(questionId) {
        // debugger;
        var span = $("[questionId='" + questionId + "']");
        $(span).show();
    }
    function showhideGroup(group) {
        var element = $("[group='" + group + "']");
        $(element).show();
    }
    function addObjChecklist(questionId, groupId, groupAlias) {
        var obj = new Object();
        obj.QuestionId = questionId;
        obj.GroupId = groupId;
        obj.GroupAlias = groupAlias;
        checkList.push(obj);
    }

    $("input[type='checkbox']").click(function () {
        var span = $(this).closest("span");

        var questionId = Number($(span).attr("questionId"));
        applyRules(questionId);
    });

    function setParentQuestionDisabled(questionId) {
        var span = $("span[questionId='" + questionId + "']:visible");
        if (span.length > 0) {
            var chckboxs = $(span).find("input[type='checkbox']");
            $(chckboxs).attr("disabled", true);
        }
    }

    function setParentQuestionUnchecked(questionId) {

        var qid = Number(questionId);

        var iFobtDependencyG26 = [455, 456, 457]; // for 454
        if ($.inArray(qid, iFobtDependencyG26) > -1) {
            var span = $("span[questionid='454']:visible");
            if (span.length > 0) {
                var checkBox = $(span).find("input[type='checkbox']");
                $(checkBox).attr("checked", false);
            }
        }

        var iFobtDependencyG3 = [419, 420, 421]; // for 454
        if ($.inArray(qid, iFobtDependencyG3) > -1) {
            var span = $("span[questionid='418']:visible");
            if (span.length > 0) {
                var checkBox = $(span).find("input[type='checkbox']");
                $(checkBox).attr("checked", false);
            }
        }

        var iFobtDependencyV4 = [542, 543, 544, 545]; // for 541
        if ($.inArray(qid, iFobtDependencyV4) > -1) {
            var span = $("span[questionid='541']:visible");
            if (span.length > 0) {
                var checkBox = $(span).find("input[type='checkbox']");
                $(checkBox).attr("checked", false);
            }
        }

    }
          
    var patientAge = '0';
    $(document).ready(function () {  
        @{
            var age = 0;
            if(Model.DoB.HasValue)
            {
                var today = DateTime.Today;
                age = today.Year - Model.DoB.Value.Year;
                if (Model.DoB.Value > today.AddYears(-age))
                {age--;}
            }
            <text>
		        patientAge = '@age';
            </text>
        }
        @if (Model != null && !Model.CheckListQuestion.IsNullOrEmpty())
        {
            //var questions = Model.CheckListQuestion.Where(x => x.Type != CheckListQuestionType.None);
            var questions = Model.CheckListQuestion;
            var groups = Model.CheckListQuestion.Select(x => x.GroupAlias).Distinct();
            var parentQuestionids = questions.Where(x => x.ParentId.HasValue && x.Type == CheckListQuestionType.CheckBox).Select(x => x.ParentId.Value).ToArray();

            foreach (var group in groups)
            {
                <text>showhideGroup("@group.ToLower()")</text>
            }

            foreach (var item in questions)
            {
                <text>addObjChecklist("@item.QuestionId","@item.GroupId","@item.GroupAlias");</text>

                if (item.Type != CheckListQuestionType.None)
                {
                    var answer = !string.IsNullOrEmpty(item.Answer) ? item.Answer.Replace("'", "\\\'").Replace("\"", "\\\"").Replace("\r", " ").Replace("\n", "{newlinefeed}") : "";

                    <text>
        showHideQuestion("@item.QuestionId")
        setQuestionStatus("@item.QuestionId", "@Html.Raw(answer)", "@item.Type")
        </text>
                }
                else
                {
                    <text>
        showHideQuestion("@item.QuestionId")
        </text>
                }
                if (parentQuestionids.Contains(item.QuestionId) && item.Type == CheckListQuestionType.CheckBox)
                {
                    <text>
        setParentQuestionDisabled("@item.QuestionId")
        </text>
                }
            }
        }

        @if (Model.IsPrintable)
        {
            <text>CopyHeaderInfoForPrint()</text>

        }
        
        @if (!Model.IsEditable)
        {
            <text>
            setDisabled();
            
            var iFOBT = $("span[questionid='" + 541 + "']:visible");
            if (iFOBT.length > 0) {
                $(iFOBT).find("input[type='checkbox']").attr('checked', '');
            }
            </text>
        }
        else
        { 
            <text>
            OsteoporosisCheck();
            TakeHomeSectioniFobtCheckV4();
            var span = $("span[questionid='" + 481 + "']:visible");
            if (span.length > 0) {
                $(span).find("input[type='checkbox']").attr('disabled', false);
            }
            </text>
        }
    });

    function OsteoporosisCheck()
    {
        var questionDependecy = [494, 495, 496, 497, 498, 499];
        if(isQuestionIdChecked(493))
        {
            disableQuestionIdCheckbox(529, true);
            $.each(questionDependecy, function (index, item) {
                disableQuestionIdCheckbox(item, true);
            });
        }
        else if(isQuestionIdChecked(529))
        {
            disableQuestionIdCheckbox(493,true);
        }
        else{
           disableQuestionIdCheckbox(529, false);
        }
    }

    function TakeHomeSectioniFobtCheckV4()
    {
        var iFobtDependency = [542, 543, 544, 545];
        var isIfobtTaken = true;
        $.each(iFobtDependency, function (index, item) {
            if (isQuestionIdChecked(item))
                isIfobtTaken = false;
        });

        if ((patientAge != '0' && patientAge >= '50' && patientAge <= '75') && isIfobtTaken)
            checkParentQuestion(541, true);
        else
            checkParentQuestion(541, false);
    }
</script>
